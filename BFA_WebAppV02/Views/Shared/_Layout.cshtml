<!DOCTYPE html>
        <html lang="en">

        <head>
            <meta charset="utf-8" />
            <meta name="viewport"
                  content="width=device-width, initial-scale=1, shrink-to-fit=no" />
            <meta name="description" content="Elegance" />
            <meta name="keywords"
                  content="Elegance,  landing launch page, html5, responsive" />
            <meta name="author" content="Schintu Design" />
            <title>@ViewBag.Title - بهگامان فراز الوند</title>

            <!-- ============ GOOGLE FONTS ============ -->
            <link href="https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,600"
                  rel="stylesheet" />

            <!-- CSS -->
            <!-- Bootstrap -->
            <link rel="stylesheet" type="text/css" href="~/Models/css/bootstrap.min.css"/>,
            <!-- Animate css -->
            <link rel="stylesheet" type="text/css" href="~/Models/css/animate.css" />
            <!-- Font Awesome CSS -->
            <link rel="stylesheet" href="~/Models/font-awesome/css/font-awesome.min.css" />
            <!-- Slider css -->
            <link rel="stylesheet" type="text/css" href="~/Models/css/slippry.css" />
            <!-- Custom styles CSS -->
            <link rel="stylesheet" type="text/css" href="~/Models/css/style.css" />
            <!-- Favicon -->
            <link rel="shortcut icon" href="~/Models/images/favicon.ico" />

            <!-- Color Template Activate only if needed -->

            <link rel="stylesheet" type="text/css" href="~/Models/css/black.css" />
            <!-- my rtl style -->
            <link rel="stylesheet" href="~/Models/css/myrtl.css" />
        </head>

        <body data-spy="scroll" data-target="#navbar" data-offset="10">
            <!-- Loading Site -->
            <div id="mask">
                <div class="loader"><img src="~/Models/images/loading.gif" alt="loading" /></div>
            </div>

            <!-- HEADER -->
            <header>
                <nav class="navbar navbar-expand-md navbar-light fixed-top">
                    <div class="container">
                        <a class="navbar-brand" href="#">
                            <!-- Logo path - use svg for higher resolution -->
                            <div class="main-logo"><img src="~/Models/images/logo01.svg" alt="" /></div>
                            Behgaman Faraz Alvand
                        </a>
                        <button class="navbar-toggler collapsed"
                                type="button"
                                data-toggle="collapse"
                                data-target="#menuCollapse"
                                aria-controls="menuCollapse"
                                aria-expanded="false"
                                aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span><i class="fa fa-ellipsis-v" aria-hidden="true"></i>
                        </button>
                        <div class="navbar-collapse collapse" id="menuCollapse">
                            <ul class="navbar-nav mr-auto"></ul>
                            <ul class="navbar-nav" id="navbar">
                                <li class="nav-item">
                                    <a class="nav-link" href="#contact">تماس با ما</a>
                                </li>
                               <!--
                                <li class="nav-item">
                                    <a class="nav-link" href="#team">تیم ما</a>
                                </li>
                                   -->

                                <li class="nav-item">
                                    <a class="nav-link" href="#portfolio">نمونه کار</a>
                                </li>
                                <!--
                                <li class="nav-item">
                                    <a class="nav-link" href="#services">خدمات</a>
                                </li>-->
                                <li class="nav-item">
                                    <a class="nav-link" href="#about-us">درباره ما</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link active" href="#home">خانه</a>
                                </li>

                            </ul>
                        </div>
                    </div>
                </nav>
            </header>








            @RenderBody()












            <!-- Scroll Up -->
            <button onclick="topFunction();" id="myBtn">
                <i class="fa fa-angle-double-up" aria-hidden="true"></i>
            </button>
            <!-- FOOTER -->
            <div class="spacer-100"></div>
            <footer id="footer">
                <div class="container">
                    <div class="row">
                        <div class="col left">
                            <div class="footer-logo">
                                <img src="~/Models/images/logo01.svg" alt="logo" /> Behgaman Faraz Alvand
                            </div>
                        </div>
                        <div class="col center">
                            <div class="footer-social">
                                <a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a>
                                <a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a><a href="#">
                                    <i class="fa fa-instagram" aria-hidden="true"></i>
                                </a>
                            </div>
                        </div>
                        <div class="col right info">
                            تمامی حقوق این سایت متعلق یه شرکت بهگامان فراز الوند است
                        </div>
                    </div>
                </div>
            </footer>
            <!-- JavaScript Files -->
            <script src="~/Models/js/jquery-3.2.1.min.js"></script>
            <script src="~/Models/js/popper.min.js"></script>
            <script src="~/Models/js/bootstrap.min.js"></script>
            <script src="~/Models/js/slippry.min.js"></script>
            <script src="~/Models/js/jquery.easing.min.js"></script>
            <script src="~/Models/js/skrollr.min.js"></script>
            <script src="~/Models/js/imagesloaded.js"></script>
            <script src="~/Models/js/isotope.pkgd.min.js"></script>
            <script src="~/Models/js/portfolio.js"></script>
            <script src="~/Models/js/load.js"></script>
            <script src="~/Models/js/map.js"></script>
            <script src="~/Models/js/functions.js"></script>
        </body>


    </html>
